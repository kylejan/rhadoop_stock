bin/hadoop jar /usr/local/hadoop/share/hadoop/tools/lib/hadoop-streaming-2.6.0.jar -input /ML_OHLC_STREAMING -output /ML_OHLC_VOLATILITY -file /usr/local/hadoop/OHLC_Volatility_streaming/stock_mapper.R -file /usr/local/hadoop/OHLC_Volatility_streaming/stock_reducer.R -mapper /usr/local/hadoop/OHLC_Volatility_streaming/stock_mapper.R -reducer /usr/local/hadoop/OHLC_Volatility_streaming/stock_reducer.R