bin/hadoop jar /usr/local/hadoop/share/hadoop/tools/lib/hadoop-streaming-2.6.0.jar -input /ML_DAY -output /ML_CHANGE_FREQUENCY -file /usr/local/hadoop/DayChangeFreq_streaming/stock_mapper.R -file /usr/local/hadoop/DayChangeFreq_streaming/stock_reducer.R -mapper /usr/local/hadoop/DayChangeFreq_streaming/stock_mapper.R -reducer /usr/local/hadoop/DayChangeFreq_streaming/stock_reducer.R